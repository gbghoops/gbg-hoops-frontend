{
	"baseUrl": "./",
	"extends": "expo/tsconfig.base",
	"strict": true,
	"allowSyntheticDefaultImports": true,
	"experimentalDecorators": true,
	"strictPropertyInitialization": false,
	"typeRoots": ["./src/types"],
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@assets/*": ["./assets/*"],
			"@src/*": ["./src/*"],
			"@/*": ["./*"]
		}
	}
}
